class Deletion element{
    public static void main(String[] args) {
int a[]= {1,18,33,45,77,96};
int index=2;
int b[]=new int[a.length-1];//new array size=4
if(index<0||index>=a.length) {
System.out.println("enter the proper index value");
}
for(int i=0;i<index;i++) {//b[0]=2,b[1]=3,
b[i]=a[i];
}
for(int i=index;i<b.length;i++) {//b[2]=5,b[3]-9
b[i]=a[i+1];
}
for(int i=0;i<b.length;i++) {
System.out.println(b[i]);
}
    }
}
